{
   "format_version":"1.16.0",
   "minecraft:entity":{
      "description":{
         "identifier":"true:supply_crate",
         "is_spawnable":false,
         "is_summonable":true,
         "is_experimental":false
      },
      "component_groups":{
         "true:default":{
            "minecraft:variant":{
               "value":0
            },
            "minecraft:timer":{
               "looping":false,
               "randomInterval":false,
               "time":11,
               "time_down_event":{
                  "event":"true:loot_dropped"
               }
            }
         },
         "true:dropped":{
            "minecraft:variant":{
               "value":1
            },
            "minecraft:interact":{
               "interactions":[
                  {
                     "hurt_item":1,
                     "on_interact":{
                        "filters":{
                           "all_of":[
                              {
                                 "test":"has_equipment",
                                 "subject":"other",
                                 "domain":"hand",
                                 "value":"true:crowbar"
                              }
                           ]
                        },
                        "event":"true:loot_open",
                        "target":"self"
                     },
                     "use_item":false,
                     "interact_text":"action.interact.open_loot_box"
                  }
               ]
            }
         },
         "true:despawn":{
            "minecraft:transformation":{
               "into":"true:despawn"
            }
         }
      },
      "components":{
         "minecraft:type_family":{
            "family":[
               "supply_drop"
            ]
         },
         "minecraft:collision_box":{
            "width":1.0,
            "height":1.0
         },
         "minecraft:health":{
            "value":500,
            "max":500
         },
         "minecraft:scale":{
            "value":0.8
         },
         "minecraft:behavior.float":{
            "priority":0
         },
         "minecraft:damage_sensor":{
            "triggers":{
               "cause":"fall",
               "deals_damage":false
            }
         },
         "minecraft:knockback_resistance":{
            "value":100,
            "max":100
         },
         "minecraft:physics":{
            
         }
      },
      "events":{
         "minecraft:entity_spawned":{
            "add":{
               "component_groups":[
                  "true:default"
               ]
            },
            "run_command":{
               "command":[
                  "execute @s ~ ~ ~ effect @s slow_falling 11 6 true",
                  "execute @p ~ ~ ~ playsound supply_crate_call"
               ]
            }
         },
         "true:loot_dropped":{
            "remove":{
               "component_groups":[
                  "true:default"
               ]
            },
            "add":{
               "component_groups":[
                  "true:dropped"
               ]
            }
         },
         "true:loot_open":{
            "add":{
               "component_groups":[
                  "true:despawn"
               ]
            },
            "run_command":{
               "command":[
                  "structure load supply_crate ~ ~ ~",
                  "execute @a [r=32] ~ ~ ~ playsound loot_drop_open"
               ]
            }
         }
      }
   }
}